import './App.css'         


function Title(){             // these is a component 
  return <h1>I am a Title!</h1>;        // we can return only a single 
}

function Description(){                     // these is a also a component 
  return <h2>I am the Description</h2>            // we are also returning
}
function App() {
  
  return (       //<Title />;             // calling a Title component
    <div>                                                              // creating a div to return multiple 
      <h1>These is use for multiple component returning</h1>
      <Description/>                             // invoking or calling the component
      <Title/>
    </div>
  );
}

export default App
